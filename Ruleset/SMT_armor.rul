  - &STR_MARINE_ARMOR                       #ARMOR TEMPLATE
    visibilityAtDay: 40
    visibilityAtDark: 35 # improved night vision but still has minor benefit from lighting
    heatVision: 20 #Add anti-smoke vision to all marine armour
    antiCamouflageAtDay: 8
    antiCamouflageAtDark: 8
    loftempsSet: [ 3 ]
    psiDefence:
      psiStrength: 0.0 #Regular marines don't use or care for any psyker abilities
      psiSkill: 1.5 #FAITH IS MY SHIELD (default is 1.0 psiStr + 0.2 psiSkill)
      moraleCurrent: 1.5 #discipline and resolve reinforces their faith
    frontArmor: 125 # base 125
    sideArmor: 120 # base 120
    rearArmor: 100 # base 100
    underArmor: 125 # base 125
    stats: # No values inherited from this, defined in each armor
      tu: 0             # base 10 
      stamina: 0        # base 20 
      health: 0         # base 10 
      bravery: 0        # base 0  
      reactions: 0      # base 10 
      firing: 0         # base 0  
      throwing: 0       # base 5  
      strength: 0       # base 10 
      psiStrength: 0    # base 0  
      psiSkill: 0       # base 0  
      melee: 0          # base 10 
