      - 1.0 #MELEE - was 1.1
      - 0.8 #ACID - was 1.0
      - 0.0 #SMOKE
      - 0.2 #IMPACT
      - 1.0 #MELTA
    tags:
      ARMOR_GAINS_PSISKILL_SECONDARY_EXPERIENCE: 2 # Gain 1-2 Devotion in any fight where this unit gains xp
      INTIMIDATION_RESISTANCE: 50
      INFECTION_RESIST: 50

