crafts:
### SPACE MARINE CRAFT
  - type: STR_SKYRANGER                     # Droppods
    requires:
      - STR_NOT_PRIMARIS
      - STR_MARINES_STRATEGY
    battlescapeTerrainData:
      name: DROPPOD_GUARD
      mapDataSets:
        - BLANKS
        - DROPPOD_GUARD
        - SMTBOMBARD
      mapBlocks:
        - name: DROPPOD_GUARD
          width: 10
          length: 20
          items:
            STR_DROPPOD_DEPLOY_CHARGE:
              - [2, 2, 0]
              - [5, 8, 0]
              - [2, 15, 0]
              - [1, 1, 1]
              - [5, 8, 1]
              - [2, 15, 1]
            STR_STORMBOLTER_TURRET_SPAWNER:
              - [1, 14, 1]
              - [4, 7, 1]
              - [1, 1, 1]
            STR_BOMBARD_SPAWNER: # 12 strikes
              - [0, 0, 5] # Bombard Platform
              - [0, 0, 5]
              - [0, 0, 5]
              - [0, 0, 5]
              - [0, 0, 5]
              - [0, 0, 5]
              - [0, 0, 5]
              - [0, 0, 5]
              - [0, 0, 5]
              - [0, 0, 5]
              - [0, 0, 5]
              - [0, 0, 5]
          fuseTimers:
            STR_DROPPOD_DEPLOY_CHARGE: [1, 1]
            STR_STORMBOLTER_TURRET_SPAWNER: [1, 1]

  - type: STR_DROPPODS                      # Firestorm Droppods
    requires:
      - STR_NOT_PRIMARIS
      - STR_ADV_DROPPODS
      - STR_MARINES_STRATEGY
    battlescapeTerrainData:
      name: DROPPOD_GUARD
      mapDataSets:
        - BLANKS
        - DROPPOD_GUARD
        - SMTBOMBARD
      mapBlocks:
        - name: DROPPOD_GUARD
          width: 10
          length: 20
          items:
            STR_BOMBARD_SPAWNER: # 12 strikes
