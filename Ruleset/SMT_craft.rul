units:
  - &STR_SMT_SMOKEL_DEFAULT                 # Craft Smokelauncher Unit
    type: STR_SMT_SMOKEL
    race: STR_SMT_SMOKEL
    stats:
      tu: 100
      stamina: 6 # 3 shots a round
      health: 60
      bravery: 110
      reactions: 200 # conceal with smoke rather than draw reactions
      firing: 100
      throwing: 0
      strength: 100
      psiStrength: 250
      psiSkill: 0
      melee: 0
      mana: 6 # 3 shots - one turn reload
    armor: STR_SMT_SMOKEL_ARMOR
    moraleLossWhenKilled: 0
    standHeight: 8 # short
    kneelHeight: 8
    deathSound: 23
    value: 0
    intelligence: 4
    aggression: 5
    sniper: 50
    spotter: 1
    energyRecovery: 34
    livingWeapon: true
    builtInWeaponSets:
      - - STR_SMT_CRAFT_SMOKE
        - STR_SMT_CRAFT_SMOKE

  - type: STR_SMT_SMOKEL_SPEEDER            # Landspeeder Smokelauncher Unit
    refNode: *STR_SMT_SMOKEL_DEFAULT
    armor: STR_SMT_SMOKEL_ARMOR_SPEEDER

  - type: STR_SMT_SMOKEL_DW                 # Craft Smokelauncher Unit (DW)
    refNode: *STR_SMT_SMOKEL_DEFAULT
    armor: STR_SMT_SMOKEL_ARMOR_DW

  - &STR_SMT_RAZORBACK_DEFAULT              # Generic Razorback Turret
    type: STR_SMT_RAZORBACK_BLANK
    race: STR_SMT_RAZORBACK
    stats:
      tu: 30 # +10, allow more spinning
      stamina: 12 # +2 over standard, enough for standard fire + 1 smoke
      health: 100
      bravery: 110
      reactions: 50 # better than default razorback, worse than droppods
      firing: 100
      throwing: 0
      strength: 100
      psiStrength: 250
      psiSkill: 0
      melee: 0
      mana: 60
    livingWeapon: true
    moraleLossWhenKilled: 0
    standHeight: 16
    kneelHeight: 16
    deathSound: 23
    value: 0 # shouldn't be costing you points when replaced
    intelligence: 4
    aggression: 5
    sniper: 50
    spotter: 1
    energyRecovery: 34
    armor: RAZORBACK_ARMOR
    liveAlien: ""

  - type: STR_SMT_RAZORBACK_BLANK           # Razorback Blank to be replaced by (Default)
    refNode: *STR_SMT_RAZORBACK_DEFAULT
    armor: STR_SMT_RAZOR_DEFAULT_ARMOR

  - type: STR_SMT_RAZORBACK_STORMBOLT       # Razorback with Stormbolter
    refNode: *STR_SMT_RAZORBACK_DEFAULT
    stats:
      stamina: 8 # -4, default weapon penalty
    armor: STR_SMT_RAZOR_SB_ARMOR
    builtInWeaponSets:
      - - STR_STORMBOLTER_TURRET_DROPPOD
        - STR_SMT_CRAFT_SMOKE

  - type: STR_SMT_RAZORBACK_HBOLTER         # Razorback with Heavy Bolter
    refNode: *STR_SMT_RAZORBACK_DEFAULT
    livingWeapon: true
    armor: STR_SMT_RAZOR_HB_ARMOR
    builtInWeaponSets:
      - - AUX_BOLTER
        - STR_SMT_CRAFT_SMOKE

  - type: STR_SMT_RAZORBACK_LASCANNON       # Razorback with Lascannon
    refNode: *STR_SMT_RAZORBACK_DEFAULT
    stats:
      mana: 50 # -10 - lascannon penalty
    armor: STR_SMT_RAZOR_LAS_ARMOR
    builtInWeaponSets:
      - - STR_SMT_CRAFT_LASCANNON
        - STR_SMT_CRAFT_SMOKE

  - type: STR_SMT_RAZORBACK_HFLAMER         # Razorback with Heavy Flamer
    refNode: *STR_SMT_RAZORBACK_DEFAULT
    stats:
      mana: 80 # +20 - flamer bonus
    armor: STR_SMT_RAZOR_HF_ARMOR
    builtInWeaponSets:
      - - STR_GORGON_HEAVY_FLAMER
        - STR_SMT_CRAFT_SMOKE

  - &STR_THUNDERHAWK_TURRET                 # Thunderhawk Turrets
    type: STR_RTT                           # Thunderhawk Turret (Left)
    race: STR_RTT
    stats:
      tu: 20
      stamina: 12
      health: 150
      bravery: 110
      reactions: 20
      firing: 120
      throwing: 0
      strength: 90
      psiStrength: 250
      psiSkill: 0
      melee: 0
      mana: 100
    standHeight: 16
    kneelHeight: 16
    value: 10 # was 25
    deathSound: 23
    intelligence: 4
    aggression: 5
    sniper: 50
    spotter: 1
    energyRecovery: 34
    moraleLossWhenKilled: 1 # was 25
    livingWeapon: true
    liveAlien: ""
    builtInWeaponSets:
      - - AUX_BOLTER
        - STR_SMT_CRAFT_SMOKE

  - type: STR_LTT                           # Thunderhawk Turret (Left)
    refNode: *STR_THUNDERHAWK_TURRET

  - type: STR_RTT_DW                        # Thunderhawk Turret Deathwatch (Right)
    refNode: *STR_THUNDERHAWK_TURRET
