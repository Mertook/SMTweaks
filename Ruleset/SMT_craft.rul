armors:
  - &STR_SMT_SMOKEL                         # Smoke Launcher (No baseplate)
    type: STR_SMT_SMOKEL_ARMOR
    spriteSheet: SLT.PCK
    spriteInv: NOTDONEIMP
    visibilityAtDay: 40
    visibilityAtDark: 40
    antiCamouflageAtDay: 5
    antiCamouflageAtDark: 5
    heatVision: 30 # vehicle standard
    personalLight: 5
    frontArmor: 150
    sideArmor: 150
    rearArmor: 150
    underArmor: 150
    movementType: 1 # flying - keep it from falling through the vehicle
    allowsMoving: false
    allowsKneeling: false
    allowInv: false
    zombiImmune: true
    bleedImmune: true
    fearImmune: true
    painImmune: true
    corpseBattle:
      - STR_BLANK_CORPSE
    drawingRoutine: 19
    constantAnimation: false
    damageModifier:
      - 1.0 #none
      - 1.0 #AP
      - 0.8 #FLAMES
      - 1.0 #HE
      - 1.0 #LASCANON
      - 1.0 #PLASMA
      - 0.0 #STUN
      - 0.8 #MELEE
      - 1.0 #ACID
      - 0.0 #SMOKE
      - 1.0 #IMPACT
      - 1.0 #MELTA
    loftempsSet: [ 3 ]
    specialWeapon: STR_TURRET_RELOAD
    tags:
      INFECTION_RESIST: 100 #infection immune
      INTIMIDATION_RESISTANCE: 100 #intimidation immune
      UNIT_TYPE_MECHANICAL: 1
      UNIT_TYPE_IMPERIUM: 1

  - type: STR_SMT_SMOKEL_ARMOR_SPEEDER            # Smoke Launcher (Landspeeder)
    refNode: *STR_SMT_SMOKEL
    spriteSheet: SLTB.PCK

  - type: STR_SMT_SMOKEL_ARMOR_DW                   # Smoke Launcher (Deathwatch)
    refNode: *STR_SMT_SMOKEL
    spriteSheet: SLTDW.PCK

  - &STR_SMT_RB_TURRET_BASE                 # Razorback Turret defaults
    type: STR_SMT_RAZOR_DEFAULT_ARMOR
    visibilityAtDay: 40
    visibilityAtDark: 40
    antiCamouflageAtDay: 10
    antiCamouflageAtDark: 10
    heatVision: 30 # vehicle standard
    personalLight: 5
    frontArmor: 150
    sideArmor: 150
    rearArmor: 150
    underArmor: 150
    movementType: 1 # flying - keep it from falling through the vehicle
    allowsMoving: false
    allowsKneeling: false
    bleedImmune: true
    fearImmune: true
    painImmune: true
    allowInv: false
    #constantAnimation: false
    scripts:
      selectUnitSprite:
        var int temp;
        unit.isCollapsing temp;
        if eq temp 0;
          unit.isAiming temp;
          if or eq blit_part blit_large_torso_0 eq blit_part blit_large_torso_1 eq blit_part blit_large_torso_2 eq blit_part blit_large_torso_3;
            if neq 0 temp;
              add sprite_offset 32;
            end;
          else or eq blit_part blit_large_propulsion_0 eq blit_part blit_large_propulsion_1 eq blit_part blit_large_propulsion_2 eq blit_part blit_large_propulsion_3;
            set sprite_index 32;
            set sprite_offset 0;
          end;
        end;
        add sprite_index sprite_offset;
        return sprite_index;
    size: 2
    spriteSheet: RB.PCK
    spriteInv: RAZOR
    drawingRoutine: 3
    corpseBattle:
      - O_CORPSE
      - O_CORPSE
      - O_CORPSE
      - O_CORPSE
    damageModifier:
      - 1.0 #none
      - 1.0 #AP
      - 0.8 #FLAMES
      - 1.0 #HE
      - 1.0 #LASCANON
      - 1.0 #PLASMA
      - 0.0 #STUN
      - 0.8 #MELEE
      - 1.0 #ACID
      - 0.0 #SMOKE
      - 1.0 #IMPACT
      - 1.0 #MELTA
    loftempsSet: [ 92, 89, 90, 91 ]
    specialWeapon: STR_TURRET_RELOAD
    tags:
      STARTS_WITH_FULL_TUS: 1
      INFECTION_RESIST: 100 #infection immune
      INTIMIDATION_RESISTANCE: 100 #intimidation immune
      UNIT_TYPE_MECHANICAL: 1
      UNIT_TYPE_IMPERIUM: 1

  - type: STR_SMT_RAZOR_DEFAULT_ARMOR       # Razorback Stormbolter
    refNode: *STR_SMT_RB_TURRET_BASE
    tags:
      STARTS_WITH_FULL_TUS: 0 # doesn't need to be marked, doesn't stay on battlefield
      SMT_RAZORBACK_TURRET: 1 # marks unit as a Razorback default turret that can be replaced

  - type: STR_SMT_RAZOR_SB_ARMOR            # Razorback Stormbolter
    refNode: *STR_SMT_RB_TURRET_BASE
    tags:
      SMT_RAZOR_SBOLTER_TURRET: 1

  - type: STR_SMT_RAZOR_HB_ARMOR            # Razorback HeavyBolter
    refNode: *STR_SMT_RB_TURRET_BASE
    tags:
      SMT_RAZOR_HBOLTER_TURRET: 1

  - type: STR_SMT_RAZOR_LAS_ARMOR           # Razorback Lascannon
    refNode: *STR_SMT_RB_TURRET_BASE
    spriteSheet: RB_LAS.PCK
    tags:
      SMT_RAZOR_LAS_TURRET: 1

  - type: STR_SMT_RAZOR_HF_ARMOR            # Razorback HeavyFlamer
    refNode: *STR_SMT_RB_TURRET_BASE
    spriteSheet: RB_HF.PCK
    tags:
      SMT_RAZOR_HFLAME_TURRET: 1

  - &STR_THUNDERHAWK_TURRET_ARMOR           # Thunderhawk Turret Base
    type: RTT_ARMOR
    spriteSheet: RTT.PCK
    drawingRoutine: 19
    spriteInv: NOTDONEIMP
    scripts:
      selectUnitSprite:
        var int temp;

        unit.isCollapsing temp;

        if eq temp 0;

          unit.getDirection sprite_offset;
          set sprite_index 0;

          unit.isAiming temp;

          if neq temp 0;

            set sprite_index 8;

          end;

        end;

        add sprite_index sprite_offset;

        return sprite_index;
    visibilityAtDay: 40
    visibilityAtDark: 40 # was 30
    heatVision: 50 # vehicle
    antiCamouflageAtDay: 10
    antiCamouflageAtDark: 10
    personalLight: 5
    frontArmor: 150
    sideArmor: 150
    rearArmor: 150
    underArmor: 150
    movementType: 1 # flying - keep it from falling through the vehicle
    allowsMoving: false
    allowsKneeling: false
    allowInv: false
    zombiImmune: true
    bleedImmune: true
    fearImmune: true
    painImmune: true
    corpseBattle:
      - STR_BLANK_CORPSE
    damageModifier:
      - 1.0 #none
      - 1.0 #AP
      - 0.8 #FLAMES
      - 1.0 #HE
      - 1.0 #LASCANON
      - 1.0 #PLASMA
      - 0.0 #STUN
      - 0.8 #MELEE
      - 1.0 #ACID
      - 0.0 #SMOKE
      - 1.0 #IMPACT
      - 1.0 #MELTA
    loftempsSet: [ 4 ]
    specialWeapon: STR_TURRET_RELOAD
    tags:
      INFECTION_RESIST: 100  # infection immune
      INTIMIDATION_RESISTANCE: 100  # intimidation immune
      UNIT_TYPE_MECHANICAL: 1
      UNIT_TYPE_IMPERIUM: 1

  - type: LTT_ARMOR
    refNode: *STR_THUNDERHAWK_TURRET_ARMOR
    spriteSheet: LTT.PCK

  - type: RTT_DW_ARMOR
    refNode: *STR_THUNDERHAWK_TURRET_ARMOR
    spriteSheet: RTTDW.PCK

  - type: LTT_DW_ARMOR
    refNode: *STR_THUNDERHAWK_TURRET_ARMOR
    spriteSheet: LTTDW.PCK

  - type: RTT_GK_ARMOR
    refNode: *STR_THUNDERHAWK_TURRET_ARMOR
    spriteSheet: RTTGK.PCK

  - type: LTT_GK_ARMOR
    refNode: *STR_THUNDERHAWK_TURRET_ARMOR
    spriteSheet: LTTGK.PCK

