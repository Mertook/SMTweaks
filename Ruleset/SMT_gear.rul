### Sonic
  - &DMG_SONIC_STD                          # Sonic Standard Damage
    type: STR_CHOIR_GUN
    hitAnimation: 120
    explosionSpeed: 1
    vaporColorSurface: {mod: 40k_ROSIGMA_edits, index: 13} # white
    vaporDensitySurface: 1
    vaporProbabilitySurface: 33
    scripts: # give it a tighter beam that disappates quickly without spreading
      vaporParticleAmmo: |
        var int temp;
        var int randMax;

        set randMax subvoxel_scale;
        muldiv randMax 2 3;
        random.randomRangeSymmetric temp randMax;
        subvoxel_offset.setX temp;
        random.randomRangeSymmetric temp randMax;
        subvoxel_offset.setY temp;
        random.randomRangeSymmetric temp randMax;
        subvoxel_offset.setZ temp;
        set temp 240;
        sub temp particle_density;
        div temp 4;
        subvoxel_velocity.setZ temp;
        mod particle_lifetime 10;
        add particle_lifetime 3;
        return;
    damageType: 14 # Sonic
    damageAlter:
      RandomType: 9 # 50-150%
      ArmorEffectiveness: 0.1
      ToArmorPre: 0.0
      ToArmor: 0.1
      ToHealth: 0.1
      ToWound: 0.15
      RandomWound: false
      ToTime: 0.2
      ToStun: 0.5
      RandomStun: false
      ToEnergy: 0.5
      ToMorale: 0.5
      ToTile: 0.25
      ToItem: 0.0
    shotgunBehavior: 1
    shotgunSpread: 66 # sizable spread
    shotgunChoke: 33
    powerRangeThreshold: 1.0
    powerRangeReduction: 1.0
    tags:
      SMT_LETHAL_STUN: 60 # 6.0x increased health damage against stun immune targets
